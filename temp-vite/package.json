{
  "name": "temp-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.minimal.config.ts",
    "build:no-check": "set NODE_OPTIONS=--no-experimental-fetch && vite build --emptyOutDir",
