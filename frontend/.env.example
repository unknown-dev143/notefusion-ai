# Environment Configuration
# Copy this file to .env.development or .env.production and update the values

# Enable/disable mock API (true/false)
USE_MOCKS=true

# API configuration
VITE_API_URL=http://localhost:5000

# Environment (development, production)
VITE_APP_ENV=development

# Debug mode (true/false)
VITE_DEBUG=false

# Mock API delay in milliseconds (0 for no delay)
VITE_MOCK_API_DELAY=200

# Logging level (debug, info, warn, error, none)
VITE_LOG_LEVEL=info

# Authentication (if needed)
# VITE_AUTH0_DOMAIN=your-auth0-domain
# VITE_AUTH0_CLIENT_ID=your-auth0-client-id
# VITE_AUTH0_AUDIENCE=your-auth0-audience

# Analytics (if needed)
# VITE_GA_TRACKING_ID=your-ga-tracking-id
