    "whatwg-fetch": "^3.6.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.9.2",
  "description": "A modern, AI-powered note-taking application with PWA support and admin capabilities.",
  "keywords": [
    "electron",
    "react",
    "vite"
  ],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.notefusion.ai",
    "productName": "NoteFusion AI",
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "public/icon.png"
    }
  }
}
