    "antd": "^5.14.1",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "draft-js": "^0.11.7",
    "electron-log": "^5.0.1",
    "electron-updater": "^6.1.7",
    "firebase": "^10.8.1",
    "html2canvas": "^1.4.1",
    "i18next": "^25.4.0",
