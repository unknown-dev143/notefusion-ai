{
  "name": "notefusion-ai-frontend",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "version": "0.1.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "generate-pwa-assets": "ts-node scripts/generate-pwa-assets.ts",
    "dev": "vite",
    "build": "vite build",
    "build:type-check": "tsc --noEmit",
    "build:vite": "vite build",
    "preview": "vite preview",
    "start": "vite preview --port 3000 --host",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postinstall": "npx prisma generate",
    "vercel-build": "npm run build",
    "electron:dev": "concurrently -k \"cross-env NODE_ENV=development vite\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "tsc -p electron.tsconfig.json && vite build",
    "electron:start": "cross-env NODE_ENV=production electron .",
    "electron:package": "npm run electron:build && electron-builder"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
