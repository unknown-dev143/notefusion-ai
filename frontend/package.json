    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-dynamic": "^1.0.3",
    "react-error-boundary": "^6.0.0",
    "react-ga4": "^2.1.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^15.7.1",
    "react-icons": "^5.5.0",
