    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-dynamic": "^1.0.3",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-quill": "^2.0.0",
